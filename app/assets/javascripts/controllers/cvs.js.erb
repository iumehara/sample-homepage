var cvsControllers = angular.module('cvsControllers', []);

cvsControllers.controller('CvIndexCtrl', ['$scope', '$location', 'cvData',
	function($scope, $location) {

		$scope.data = cvData.data;

		cvData.loadData();

		$scope.navbarPartial = {
			url: "<%= asset_path '_navbar.html' %>"
		};

		$scope.feedbackForm = {
			url: "<%= asset_path '_feedbackForm.html' %>"
		};

		$scope.navHome = function() {
			$location.url('/')
		}
	}
]);

